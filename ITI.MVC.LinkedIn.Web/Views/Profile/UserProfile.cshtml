@model ITI.MVC.LinkedIn.Web.Models.ViewModels.ProfileVM
@using ITI.MVC.LinkedIn.Web.Models.ViewModels
@{
    int? coursesCount = 0;
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" />
    <link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css">
    <script src="https://kit.fontawesome.com/5028f5981e.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="~/Content/HossamCSS/style.css" />
    
     <title>LinkedIn</title>
</head>

<body >
    <!-- Profile Header Section -->
    <section class="p-3" id="ProfileSummary">
        <div class="container u-border">
            <div class="row">
                <!-- upper div that includes img -->
                <div class="col-sm-12 profile-imgs">
                    <!--profile img -->
                    @if(ViewBag.imag!=null){
                    <div class="position-absolute profile-img">
                        <img src="~/Content/images/@ViewBag.imag" class="rounded-circle">
                    </div>
                    }
                    else
                    {
                        <div class="position-absolute profile-img">
                            <img src="~/Content/images/img_184513204708457.png" class="rounded-circle">
                        </div>
                    }
                </div>
                <!-- down div -->
                <div class="col-sm-12">
                    <!-- upper section -->
                    <div class="row pt-sm-3 pb-sm-3">
                        <div class="col-sm-6 offset-6 dropdown mx-50">
                            <div class="d-flex justify-content-end align-items-center edit-profile">
                                <div class="p-2">
                                    <a role="button" href="#" class="dropdown-toggle btn-primary btn-lg add-section"
                                       id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                        Add Profile Section <!-- <i class="fas fa-sort-down"></i> -->
                                    </a>
                                    <!-- Add Profile Section Modal -->
                                    <div class="dropdown-menu dropdown-menu-width " aria-labelledby="dropdownMenu1">
                                        <!-- Add Profile Section form -->
                                        <form class="accordion list  " id="accordionExample">
                                            <!-- intro card -->
                                            <div class="card">
                                                <div class="card-header" id="headingOne">
                                                    <h2 class="mb-0">
                                                        <a class="btn btn-link" type="button" data-toggle="collapse" data-target="#collapseOne"
                                                           aria-expanded="true" aria-controls="collapseOne">
                                                            <span class="card-title"> Intro</span>
                                                        </a>
                                                    </h2>
                                                </div>
                                                <div id="collapseOne" class="collapse show" aria-labelledby="headingOne"
                                                     data-parent="#accordionExample">
                                                    <div class="card-body">
                                                        <div>
                                                            <p class="font-weight-normal">
                                                                Help the right people find you by showing what opportunities
                                                                you’re open to.
                                                            </p>
                                                        </div>
                                                        <div style=" display: flex; align-items: center; font-size: 2rem;">
                                                            <i class="fab fa-stumbleupon-circle p-2"></i>
                                                            <p class="p-2 font-weight-light">
                                                                Looking for job opportunities Show recruiters and others
                                                                you’re open to job opportunities.
                                                            </p>
                                                            <a> <i class="far fa-plus-square p-2"></i></a>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <!-- About card -->
                                            <div class="card">
                                                <div class="card-header d-flex justify-content-between " id="headingTwo">
                                                    <!-- <h2 class="mb-0 justify-content-between "> -->
                                                    <div class="p-2">
                                                        <a class="btn btn-link collapsed text-reset" type="button" data-toggle="collapse"
                                                           data-target="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                                                            About
                                                        </a>
                                                    </div>
                                                    <div class="p-2" style="margin: auto 0;">
                                                        <i class="fas fa-chevron-down "></i>
                                                    </div>
                                                    <!-- </h2> -->
                                                </div>
                                                <div id="collapseTwo" class="collapse" aria-labelledby="headingTwo"
                                                     data-parent="#accordionExample">
                                                    <div class="card-body">
                                                        @*About Summary*@
                                                        <a class="work text-reset" href="#">
                                                            <div class="d-flex m-auto p-2">
                                                                <div class="p-2" style=" display: flex; align-items: center; ">
                                                                    <i class="fas fa-indent"></i>
                                                                    <div class="d-block">
                                                                        <p>Add Summary </p>
                                                                        <p class=" font-weight-light p-0 m-0">Use summary to share what you do or the opportunities you’re looking for.</p>

                                                                    </div>
                                                                </div>
                                                                <div class="p-2" style="margin: auto 0;">
                                                                    <a href="" class="col-sm-1 text-right " data-toggle="modal" data-target="#Summary">
                                                                        <i class="fas fa-plus p-3 profile-icon "></i>
                                                                    </a>
                                                                </div>
                                                            </div>
                                                        </a>


                                                    </div>
                                                </div>
                                            </div>
                                            <!-- Background card -->
                                            <div class="card">
                                                <div class="card-header" id="headingThree">
                                                    <h2 class="mb-0">
                                                        <a class="btn btn-link collapsed text-reset" type="button" data-toggle="collapse"
                                                           data-target="#collapseThree" aria-expanded="false" aria-controls="collapseThree">
                                                            Background
                                                        </a>
                                                    </h2>
                                                </div>
                                                <div id="collapseThree" class="collapse" aria-labelledby="headingThree"
                                                     data-parent="#accordionExample">
                                                    <div class="card-body" style="font-size: 2rem;">
                                                        @*workExperince*@
                                                        <a class="work text-reset" href="#">
                                                            <div class="d-flex m-auto p-2">
                                                                <div class="p-2" style=" display: flex; align-items: center; ">
                                                                    <i class="fas fa-briefcase" style="margin-right: 1rem;"></i>
                                                                    <div class="d-block">
                                                                        <p>Work experience</p>
                                                                        <p class=" font-weight-light p-0 m-0">3 positions on your profile.</p>

                                                                    </div>
                                                                </div>
                                                                <div class="p-2" style="margin: auto 0;">
                                                                    <a href="" onclick="addEperience( )" class="col-sm-1 text-right " data-toggle="modal" data-target="#myModalTest">
                                                                        <i class="fas fa-plus p-3 profile-icon "></i>
                                                                    </a>
                                                                </div>
                                                            </div>
                                                        </a>
                                                        @*Education*@
                                                        <a class="education text-reset " href="# ">
                                                            <div class="d-flex m-auto p-2">
                                                                <div class="p-2" style="display: flex; align-items: center; ">
                                                                    <i class="fas fa-university" style="margin-right: 1rem;"></i>
                                                                    <div class="d-block">
                                                                        <p>Education</p>
                                                                        <p class=" font-weight-lighter p-0 m-0">1 school in your profile.</p>

                                                                    </div>
                                                                </div>
                                                                <div class="p-2" style="margin: auto 0;">
                                                                    <a href="#" onclick="addEducation()" class="col-sm-1 text-right " data-toggle="modal" data-target="">
                                                                        <i class="fas fa-plus p-3 profile-icon "></i>
                                                                    </a>
                                                                </div>
                                                            </div>
                                                        </a>

                                                        @*Licenses*@
                                                        <a class="licenses  text-reset" href="# ">
                                                            <div class="d-flex m-auto p-2">

                                                                <div class="p-2" style=" display: flex; align-items: center; ">

                                                                    <i class="fas fa-pager" style="margin-right: 1rem;"></i>
                                                                    <div class="d-block">
                                                                        <p>Licenses & Certifications</p>
                                                                        <p class=" font-weight-light p-0 m-0">
                                                                            Showcase your expertise with your credentials.
                                                                        </p>

                                                                    </div>
                                                                </div>
                                                                <div class="p-2" style="margin: auto 0;">
                                                                    <a href="#" onclick="()" class="col-sm-1 text-right " data-toggle="modal" data-target="#">
                                                                        <i class="fas fa-plus p-3 profile-icon "></i>
                                                                    </a>
                                                                </div>
                                                            </div>
                                                        </a>
                                                        @*volunteer*@
                                                        <a class="volunteer  text-reset" href="# ">
                                                            <div class="d-flex j m-auto p-2">
                                                                <div class="p-2" style=" display: flex; align-items: center; ">
                                                                    <i class="far fa-heart" style="margin-right: 1rem;"></i>
                                                                    <div class="d-block">
                                                                        <p>Volunteer experience</p>
                                                                        <p class=" font-weight-light p-0 m-0">Highlight your passions and how you like to give back</p>
                                                                    </div>
                                                                </div>
                                                                <div class="p-2" style="margin: auto 0;">
                                                                    <a href="#" onclick="AddVolunteer()" class="col-sm-1 text-right " data-toggle="modal" data-target="#">
                                                                        <i class="fas fa-plus p-3 profile-icon "></i>
                                                                    </a>
                                                                </div>
                                                            </div>
                                                        </a>

                                                    </div>
                                                </div>
                                            </div>
                                            <!-- skills card -->
                                            <div class="card">
                                                <div class="card-header d-flex justify-content-between " id="headingFour">
                                                    <!-- <h2 class="mb-0 justify-content-between "> -->
                                                    <div class="p-2">
                                                        <a class="btn btn-link collapsed text-reset" type="button" data-toggle="collapse"
                                                           data-target="#collapseFour" aria-expanded="false" aria-controls="collapseTwo">
                                                            Skills
                                                        </a>
                                                    </div>
                                                    <div class="p-2" style="margin: auto 0;">
                                                        <i class="fas fa-chevron-down "></i>
                                                    </div>
                                                    <!-- </h2> -->
                                                </div>
                                                <div id="collapseFour" class="collapse" aria-labelledby="headingFour"
                                                     data-parent="#accordionExample">
                                                    <div class="card-body">
                                                        @*Skills*@

                                                        <a class="volunteer  text-reset" href="# ">
                                                            <div class="d-flex j m-auto p-2">
                                                                <div class="p-2" style=" display: flex; align-items: center; ">
                                                                    <i class="fas fa-medal"></i>
                                                                    <div class="d-block">
                                                                        <p>Skills</p>
                                                                        <p class=" font-weight-light p-0 m-0">Add Skills</p>
                                                                    </div>
                                                                </div>
                                                                <div class="p-2" style="margin: auto 0;">
                                                                    <a href="" class="col-sm-1 text-right " data-toggle="modal" data-target="#SkillsModal">
                                                                        <i class="fas fa-plus p-3 profile-icon "></i>
                                                                    </a>
                                                                </div>
                                                            </div>
                                                        </a>

                                                    </div>
                                                </div>
                                            </div>
                                            <!-- accomplishment -->
                                            <div class="card">
                                                <div class="card-header d-flex justify-content-between " id="headingFive">
                                                    <!-- <h2 class="mb-0 justify-content-between "> -->
                                                    <div class="p-2">
                                                        <a class="btn btn-link collapsed text-reset" type="button" data-toggle="collapse"
                                                           data-target="#collapseFive" aria-expanded="false" aria-controls="collapseFive">
                                                            Acccomplishment
                                                        </a>
                                                    </div>
                                                    <div class="p-2" style="margin: auto 0;">
                                                        <i class="fas fa-chevron-down "></i>
                                                    </div>
                                                    <!-- </h2> -->
                                                </div>
                                                <div id="collapseFive" class="collapse" aria-labelledby="headingFive"
                                                     data-parent="#accordionExample">
                                                    @*publications*@
                                                    <a class="work text-reset" href="#">
                                                        <div class="d-flex m-auto p-2">
                                                            <div class="p-2" style=" display: flex; align-items: center; ">
                                                                <i class="fas fa-briefcase" style="margin-right: 1rem;"></i>
                                                                <div class="d-block">
                                                                    <p>Publications</p>
                                                                    <p class=" font-weight-light p-0 m-0">List your published work and be found 7x more</p>

                                                                </div>
                                                            </div>
                                                            <div class="p-2" style="margin: auto 0;">
                                                                <a href="" class="col-sm-1 text-right " data-toggle="modal" data-target="#Publications">
                                                                    <i class="fas fa-plus p-3 profile-icon "></i>
                                                                </a>
                                                            </div>
                                                        </div>
                                                    </a>
                                                    @*patents*@
                                                    <a class="work text-reset" href="#">
                                                        <div class="d-flex m-auto p-2">
                                                            <div class="p-2" style=" display: flex; align-items: center; ">
                                                                <i class="fas fa-briefcase" style="margin-right: 1rem;"></i>
                                                                <div class="d-block">
                                                                    <p>Patents</p>
                                                                    <p class=" font-weight-light p-0 m-0">Showcase your innovation and expertise</p>

                                                                </div>
                                                            </div>
                                                            <div class="p-2" style="margin: auto 0;">
                                                                <a href="" class="col-sm-1 text-right " data-toggle="modal" data-target="#Patents">
                                                                    <i class="fas fa-plus p-3 profile-icon "></i>
                                                                </a>
                                                            </div>
                                                        </div>
                                                    </a>
                                                    @*courses*@
                                                    <a id="Courses" class="work text-reset" href="#">
                                                        <div class="d-flex m-auto p-2">
                                                            <div class="p-2" style=" display: flex; align-items: center; ">
                                                                <i class="fas fa-briefcase" style="margin-right: 1rem;"></i>
                                                                <div class="d-block">
                                                                    <p>Courses</p>
                                                                    <p class=" font-weight-light p-0 m-0">1 course on your profile</p>

                                                                </div>
                                                            </div>
                                                            <div class="p-2" style="margin: auto 0;">
                                                                <a id="course-button" class="col-sm-1 text-right " data-toggle="modal" data-target="#">
                                                                    <i class="fas fa-plus p-3 profile-icon "></i>
                                                                </a>
                                                            </div>
                                                        </div>
                                                    </a>
                                                    @*projects*@
                                                    <a class="work text-reset" href="#">
                                                        <div class="d-flex m-auto p-2">
                                                            <div class="p-2" style=" display: flex; align-items: center; ">
                                                                <i class="fas fa-briefcase" style="margin-right: 1rem;"></i>
                                                                <div class="d-block">
                                                                    <p>Projects</p>
                                                                    <p class=" font-weight-light p-0 m-0">Add compelling projects to demonstrate your experience</p>

                                                                </div>
                                                            </div>
                                                            <div class="p-2" style="margin: auto 0;">
                                                                <a href="" id="project-btn" col-sm-1 text-right btn " data-toggle="modal" data-target="">
                                                                    <i class="fas fa-plus p-3 profile-icon "></i>
                                                                </a>
                                                            </div>
                                                        </div>
                                                    </a>
                                                    @*Honor and rewards*@
                                                    <a class="work text-reset" href="#">
                                                        <div class="d-flex m-auto p-2">
                                                            <div class="p-2" style=" display: flex; align-items: center; ">
                                                                <i class="fas fa-briefcase" style="margin-right: 1rem;"></i>
                                                                <div class="d-block">
                                                                    <p>Honor and Rewards</p>
                                                                    <p class=" font-weight-light p-0 m-0">Feature the recognition you’ve earned</p>

                                                                </div>
                                                            </div>
                                                            <div class="p-2" style="margin: auto 0;">
                                                                <a href="" id="award-btn" class="col-sm-1 text-right btn" data-toggle="modal" data-target="">
                                                                    <i class="fas fa-plus p-3 profile-icon "></i>
                                                                </a>
                                                            </div>
                                                        </div>
                                                    </a>
                                                    @*Test Scores*@
                                                    <a class="work text-reset" href="#">
                                                        <div class="d-flex m-auto p-2">
                                                            <div class="p-2" style=" display: flex; align-items: center; ">
                                                                <i class="fas fa-briefcase" style="margin-right: 1rem;"></i>
                                                                <div class="d-block">
                                                                    <p>Test Scores</p>
                                                                    <p class=" font-weight-light p-0 m-0">If you excelled in an exam, you can list your score here</p>

                                                                </div>
                                                            </div>
                                                            <div class="p-2" style="margin: auto 0;">
                                                                <a id="testScore-add-button" href="" class="col-sm-1 text-right " data-toggle="modal" data-target="">
                                                                    <i class="fas fa-plus p-3 profile-icon "></i>
                                                                </a>
                                                            </div>
                                                        </div>
                                                    </a>
                                                    @*languages*@
                                                    <a class="work text-reset" href="#">
                                                        <div class="d-flex m-auto p-2">
                                                            <div class="p-2" style=" display: flex; align-items: center; ">
                                                                <i class="fas fa-briefcase" style="margin-right: 1rem;"></i>
                                                                <div class="d-block">
                                                                    <p>Languages</p>
                                                                    <p class=" font-weight-light p-0 m-0">Add Laungauge</p>

                                                                </div>
                                                            </div>
                                                            <div class="p-2" style="margin: auto 0;">
                                                                <a id="User-langauge-btn" href="" class="col-sm-1 text-right " data-toggle="modal" data-target="">
                                                                    <i class="fas fa-plus p-3 profile-icon "></i>
                                                                </a>
                                                            </div>
                                                        </div>
                                                    </a>
                                                    @*Organizations*@
                                                    <a class="work text-reset" href="#">
                                                        <div class="d-flex m-auto p-2">
                                                            <div class="p-2" style=" display: flex; align-items: center; ">
                                                                <i class="fas fa-briefcase" style="margin-right: 1rem;"></i>
                                                                <div class="d-block">
                                                                    <p>Organizations</p>
                                                                    <p class=" font-weight-light p-0 m-0">Show your involvement with communities that are important to you</p>

                                                                </div>
                                                            </div>
                                                            <div class="p-2" style="margin: auto 0;">
                                                                <a href="" class="col-sm-1 text-right " data-toggle="modal" data-target="#Organization">
                                                                    <i class="fas fa-plus p-3 profile-icon "></i>
                                                                </a>
                                                            </div>
                                                        </div>
                                                    </a>
                                                </div>
                                            </div>
                                            <!-- Supported language -->
                                            <div class="card">
                                                <div class="card-header d-flex justify-content-between " id="headingSix">
                                                    <!-- <h2 class="mb-0 justify-content-between "> -->
                                                    <div class="p-2">
                                                        <a class="btn btn-link collapsed text-reset" type="button" data-toggle="collapse"
                                                           data-target="#collapseSix" aria-expanded="false" aria-controls="collapseSix">
                                                            Supported & Language
                                                        </a>
                                                    </div>
                                                    <div class="p-2" style="margin: auto 0;">
                                                        <i class="fas fa-chevron-down "></i>
                                                    </div>
                                                    <!-- </h2> -->
                                                </div>
                                                <div id="collapseSix" class="collapse" aria-labelledby="headingSix"
                                                     data-parent="#accordionExample">
                                                    <div class="card-body">

                                                        <div style=" display: flex;  justify-content:space-between; align-items: center; font-size: 2rem;">
                                                            <i class="fas fa-globe"></i>
                                                            <div class="d-block">
                                                                <p>Add profile IN another language</p>
                                                                <p class=" font-weight-lighter p-0 m-0">
                                                                    You can add the above sections in other languages to make it easier for people to find
                                                                    you.
                                                                </p>
                                                            </div>
                                                            <div class="p-2" style="margin: auto 0;">
                                                                <i class="far fa-plus-square"></i>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </form>
                                    </div>

                                </div>
                                <div class="dropdown p-2">
                                    <a class="btn border border-dark btn-light" id="dropdownMenu2" data-toggle="dropdown"
                                       aria-haspopup="true" aria-expanded="false">
                                        More....
                                    </a>
                                    <div class="dropdown-menu" aria-labelledby="dropdownMenu2">
                                        <a class="dropdown-item">
                                            <i class="fas fa-share p-2"></i>Share Profile vs
                                            Message
                                        </a>
                                        <a class="dropdown-item"> <i class="fas fa-share p-2"></i>Save as PDF</a>
                                    </div>
                                </div>
                                <div class="p-2">
                                    <a href="#"><i class="fas fa-pencil-alt"></i></a>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- down section  -->
                    <div class="row p-3">
                        <div class="col-sm-6 user-info">
                            <div class="p-sm-3">
                                <div>
                                    <h2 class="d-inline-block">Nora </h2>
                                    <h2 class="d-inline-block"> Mohamed</h2>
                                </div>
                                <div>
                                    <h4> ITI Intake 40</h4>
                                </div>
                                <div class="d-flex">
                                    <h4>@ViewBag.country </h4>
                                    <ul class="info">
                                        <!-- Button trigger modal -->
                                        <li id="connection"><a href="#" class="btn btn-link"> @ViewBag.connection+ Connections </a></li>
                                        <li id="contact">
                                            <a href="#" type="button" class="btn btn-link" data-toggle="modal" data-target="#Connection">
                                                Contact info
                                            </a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6 p-sm-3 experiences">
                            <div class="d-flex justify-content-end align-items-center">
                                <div class="sm-img"><img class="img-fluid" src="~/Content/images/itiLogo.png"> </div>
                                <div> <a href="#" class="btn btn-link"> Information Technology Instutute ITI </a></div>
                            </div>
                            <div class="d-flex justify-content-end align-items-center">
                                <div class="sm-img"><img class="img-fluid" src="~/Content/images/itiLogo.png"> </div>
                                <div> <a href="#" class="btn btn-link"> Information Technology Instutute ITI </a></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- About section -->
    <section class="p-3" id="About">
        <div class="container">
            <div class="row u-border pt-5 pb-5 pr-3 pl-3">
                <header class="col-12 d-flex justify-content-between">
                    <h2>About</h2>
                    <a href="#" data-toggle="modal" data-target="#Summary">
                        <i class="fas fa-pencil-alt profile-icon"></i>
                    </a>
                </header>
                <div class="col-12 pt-3">
                    <h5>
                        Talented, Skilled, Passionate, and even Young Hardcore Developer
                    </h5>
                </div>
            </div>
        </div>
    </section>

    <!-- Dashboard section -->
    <section class="p-3" id="Dashboard">
        <div class="container">
            <div class="row dashboard u-border">
                <h2 class="dashboard__title">Your Dashboard</h2>
                <h3 class="dashboard__subtitle">Private to you</h3>
                <div class="list-group list-group-horizontal flex-fill">
                    <a href="#" class="list-group-item dashboard__item list-group-item-action">
                        <div class="dashboard__count">4</div>
                        <div class="dashboard__description">Who viewed your profile</div>
                    </a>
                    <a href="#" class="list-group-item dashboard__item list-group-item-action">
                        <div class="dashboard__count">4</div>
                        <div class="dashboard__description">Who viewed your profile</div>
                    </a>
                    <a href="#" class="list-group-item dashboard__item list-group-item-action">
                        <div class="dashboard__count">4</div>
                        <div class="dashboard__description">Who viewed your profile</div>
                    </a>
                </div>
            </div>
        </div>
    </section>

    <!-- background section -->
    <section class="p-3" id="Background">
        <div class="background container u-border rounded p-0">
